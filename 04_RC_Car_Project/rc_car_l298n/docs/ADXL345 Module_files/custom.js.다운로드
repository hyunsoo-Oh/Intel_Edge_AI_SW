      "a PIR sensor",
      "ultrasonic sensors",
      "PIR sensor module",
      "the reed switch module",
      "the touch switch module",
      "IR obstacle avoidance module",
      "SunFounder RGB Matrix module",
      "a power supply",
      "an Micro SD",
      "the screen",
      "a USB keyboard and a USB mouse",
      "a case",
      "Arduino UNO",
    ];
  let localUrl = location.href;
  var curIndex = [];
  // 获取当前页面在数据中的位置
  arrPage.forEach(function (item, index) {
    if (item == localUrl) {
      curIndex.push(index);
    }
  });
  let curText = null;
  // 获取所有关键字字段
  let allP = document.getElementsByTagName("p");
  allP = [...allP];
  // 获取页面关键字的长度
  let keyWordLength = 0;
  // 添加标签
  allP.forEach(function (item, index) {
    if (curIndex.length > 0) {
      let _item = item;
      curIndex.forEach(function (item) {
        curText = arrText[item];
        if (_item.innerText.includes(curText)) {
          keyWordLength = keyWordLength + 1;
          _item.innerText = _item.innerText.replace(
            curText,
            `<a href="${arrLink[item]}" target="_blank">${curText}</a>`
          );
          _item.innerHTML = _item.innerText;
        }
      });
    }
  });
};

/* 设置分享
 * 添加分享标签
 */
var addShareTag = function () {
  // <script type='text/javascript' src='https://platform-api.sharethis.com/js/sharethis.js#property=64867e50ab7e090012d1dca6&product=sop' async='async'></script>
  let _script = document.createElement('script');
  _script.type = "text/javascript";
  _script.src = "https://platform-api.sharethis.com/js/sharethis.js#property=64867e50ab7e090012d1dca6&product=sop";
  _script.async = "async";
  document.getElementsByTagName('body')[0].appendChild(_script);
}

/* 
 * 添加追踪网站访问者行为
 */
var addFacebookPixel = function () {
  // 创建<script>元素
  var script = document.createElement('script');
  script.innerHTML = `
      !function(f,b,e,v,n,t,s)
      {if(f.fbq)return;n=f.fbq=function(){n.callMethod?
      n.callMethod.apply(n,arguments):n.queue.push(arguments)};
      if(!f._fbq)f._fbq=n;n.push=n;n.loaded=!0;n.version='2.0';
      n.queue=[];t=b.createElement(e);t.async=!0;
      t.src=v;s=b.getElementsByTagName(e)[0];
      s.parentNode.insertBefore(t,s)}(window, document,'script',
      'https://connect.facebook.net/en_US/fbevents.js');
      fbq('init', '3626774964229244');
      fbq('track', 'PageView');
  `;

  // 将<script>元素添加到<head>标签中
  document.head.appendChild(script);

  // 创建<noscript>元素
  var noscript = document.createElement('noscript');
  noscript.innerHTML = `
      <img height="1" width="1" style="display:none"
      src="https://www.facebook.com/tr?id=3626774964229244&ev=PageView&noscript=1"/>
  `;

  // 将<noscript>元素添加到<head>标签中
  document.head.appendChild(noscript);
}

/* 
 * 统计访客数据
 */
var addGoogleTag = function () {
  // 创建 script 标签并设置属性
  var gtagScript = document.createElement('script');
  gtagScript.async = true;
  gtagScript.src = 'https://www.googletagmanager.com/gtag/js?id=G-B947WJSNKS';

  // 创建另一个 script 标签来初始化 gtag
  var initScript = document.createElement('script');
  initScript.innerHTML = `
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());
    gtag('config', 'G-B947WJSNKS');
  `;

  // 将 script 标签添加到 head 中
  document.head.appendChild(gtagScript);
  document.head.appendChild(initScript);
}


window.onload = function () {
  setNavBar();
  removeFooterRTDInfo();
  // setKeyword(); (Deprecated 弃用)
  addShareTag();
  addFacebookPixel();
  addGoogleTag();
};
setPageLogo();
